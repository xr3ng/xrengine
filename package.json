{
  "name": "xrengine",
  "description": "A free, open source, scalable social WebXR engine for everyone.",
  "version": "0.0.8",
  "homepage": "http://github.com/xr3ngine/xrengine",
  "repository": "http://github.com/xr3ngine/xrengine",
  "private": false,
  "main": "src",
  "keywords": [
    "xrengine"
  ],
  "author": {
    "name": "xr3ngine",
    "email": "info@xrengine.io"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^13.0.0",
    "yarn": ">= 0.18.0"
  },
  "bin": {
    "xrengine": "./cli.mjs"
  },
  "scripts": {
    "postinstall": "(cd client && yarn install) && (cd docs && npm install)",
    "compile": "shx rm -rf lib/ && tsc",
    "dev": "ts-node-dev --no-notify src/",
    "dev-db": "cd scripts && sudo bash ./start-db.sh",
    "dev-reinit-db": "NODE_ENV=dev FORCE_DB_REFRESH=true ts-node-dev --no-notify src/",
    "dev-generate-fakes": "NODE_ENV=dev env GENERATE_FAKES=true ts-node-dev --no-notify src/",
    "lint": "node node_modules/.bin/eslint --ext .js,.ts src/ test/ --fix",
    "mocha": "NODE_ENV=test ts-mocha \"test/**/*.ts\" --recursive --exit",
    "start": "yarn run compile && node lib/",
    "sync": "PERFORM_DRY_RUN=true FORCE_DB_REFRESH=true node lib/",
    "test": "yarn run lint && yarn run compile && yarn run mocha",
    "precommit": "no-master-commits -b master,dev",
    "compile-with-client": "(npm run compile) & (cd client && npm run start)",
    "dev-with-client": "(npm run dev) & (cd client && npm run dev 3000)",
    "lint-with-client": "(npm run lint) & (cd client && npm run lint)",
    "start-with-client": "(npm run start) & (cd client && npm run start)",
    "test-with-client": "(npm run test) & (cd client && npm run test)",
    "docs": "(cd docs && hugo)"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "resolutions": {
    "is-promise": "2.1.0",
    "simple-swizzle/is-arrayish/is-promise": "2.1.0",
    "fs-capacitor/is-promise": "2.1.0"
  },
  "types": "lib/",
  "pre-push": [
    "test"
  ],
  "pre-commit": [
    "test",
    "precommit"
  ],
  "peerDependencies": {
    "graphql": "^14.0.0",
    "graphql-relay": "~0.5.5",
    "graphql-sequelize": "~9.4.0",
    "sequelize": "~5.21.8",
